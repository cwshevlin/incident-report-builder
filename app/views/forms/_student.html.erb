<%= form_for(@student) do |f| %>
    <p>
      <%= f.label "First Name" %>
      <%= f.text_field :first_name %>
    </p>
    <p>
      <%= f.label "Last Name" %>
      <%= f.text_field :last_name %>
    </p>
    <p>
      <%= f.label "Grade" %>
      <%= f.text_field :grade %>
    </p>
<%= submit_tag nil, class: "btn btn-primary" %>

<% end %>