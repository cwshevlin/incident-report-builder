<%= form_for(@teacher) do |f| %>
    <p>
      <%= f.label "First Name" %>
      <%= f.text_field :first_name %>
    </p>
    <p>
      <%= f.label "Last Name" %>
      <%= f.text_field :last_name %>
    </p>
    <p>
      <%= f.label "Email" %>
      <%= f.text_field :email %>
    </p>
<%= submit_tag nil, class: "btn btn-primary" %>

<% end %>